{
    "name": "decodelabs/greenleaf",
    "description": "Super-fast directory based HTTP router",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/atlas": "^0.13.1",
        "decodelabs/archetype": "^0.3.10",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/dictum": "^0.6.10",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/harvest": "^0.6",
        "decodelabs/iota": "^0.2.4",
        "decodelabs/monarch": "^0.1.4",
        "decodelabs/nuance": "^0.1.2",
        "decodelabs/singularity": "^0.3.4",
        "decodelabs/slingshot": "^0.2.1",
        "decodelabs/veneer": "^0.12.8",

        "psr/container": "^2.0.2",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0.2",
        "psr/http-server-middleware": "^1.0.2"
    },
    "require-dev": {
        "decodelabs/commandment": "^0.2.2",
        "decodelabs/guidance": "^0.2.5",
        "decodelabs/pandora": "^0.3.3",
        "decodelabs/phpstan-decodelabs": "^0.7",
        "decodelabs/terminus": "^0.13.1"
    },
    "conflict": {
        "decodelabs/guidance": "<0.2",
        "decodelabs/pandora": "<0.3"
    },
    "suggest": {
        "decodelabs/guidance": "Uuid route support"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Greenleaf\\": "src/Greenleaf/"
        },
        "classmap": [
            "src/Archetype/",
            "src/Commandment/",
            "src/Harvest/",
            "src/Singularity/"
        ],
        "files": [
            "src/Greenleaf/Context.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Greenleaf\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.8.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
